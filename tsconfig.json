{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": ["env.d.ts", "src/**/*", "src/**/*.vue","vite.config.*", "vitest.config.*", "cypress.config.*", "playwright.config.*","src/stores/file.json"],
  "compilerOptions": {
    "target": "ES2015",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "composite": true,
    "types": ["node","vite/client"],
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    }
  }
}
